				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="Url" type="xsd:anyURI">
				<xsd:annotation>
					<xsd:documentation>Web page associated with connection link.
						DetailLevel=full</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
