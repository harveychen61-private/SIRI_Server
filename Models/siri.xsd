	<!-- === VIEW of SCHEDULED CONNECTION LINK ============================================== -->
	<xsd:element name="AnnotatedConnectionLinkRef" type="AnnotatedConnectionLinkStructure">
		<xsd:annotation>
			<xsd:documentation>SCHEDULED CONNECTION LINK definition.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="AnnotatedConnectionLinkStructure">
		<xsd:annotation>
			<xsd:documentation>View of a SCHEDULED CONNECTION LINK description.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element form="qualified" name="ConnectionLinkRef" type="ConnectionLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Identifer of the connection link. DetailLevel=minimum
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="FeederStopPointRef"
				type="StopPointRefStructure">
				<xsd:annotation>
					<xsd:documentation>Identifer of the feeder's stop. DetailLevel=normal
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="DistributorStopPointRef"
				type="StopPointRefStructure">
				<xsd:annotation>
					<xsd:documentation>Identifer of the distributor's stop. DetailLevel=normal
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element default="true" form="qualified" minOccurs="0" name="Monitored"
				type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Whether real-time data is available for the connection link.
						Default is 'true'. DetailLevel=minimum </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0"
				name="ConnectionLinkName" type="NaturalLanguageStringStructure">
				<xsd:annotation>
					<xsd:documentation>Name of SCHEDULED CONNECTION LINK. DetailLevel=minimum
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0"
				name="FeederStopPointName" type="NaturalLanguageStringStructure">
				<xsd:annotation>
					<xsd:documentation>Name of the feeder's stop. DetailLevel=full
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0"
				name="DistributorStopPointName" type="NaturalLanguageStringStructure">
				<xsd:annotation>
					<xsd:documentation>Name of the distributor's stop.
