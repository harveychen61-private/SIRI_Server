	<xsd:group name="AllFacilitiesGroup">
		<xsd:annotation>
			<xsd:documentation>Facilities that apply to stops.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="ServiceFacilityGroup"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AssistanceFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="HireFacility"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="CommonFacilityGroup">
		<xsd:annotation>
			<xsd:documentation>tructured Classification Elements. Corresponds to TPEG 18 Event
				Reason.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="FareClassFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="TicketingFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="NuisanceFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="MobilityFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="PassengerInformationFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="PassengerCommsFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="RefreshmentFacility"/>
			<xsd:element default="unknown" form="qualified" maxOccurs="unbounded" minOccurs="0"
				name="AccessFacility" type="AccessFacilityEnumeration">
				<xsd:annotation>
					<xsd:documentation>Classification of Access Facility.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="SanitaryFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="LuggageFacility"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="StopFacilityGroup">
		<xsd:annotation>
			<xsd:documentation>Facilities that apply to stops.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="CommonFacilityGroup"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AssistanceFacility"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="HireFacility"/>
		</xsd:sequence>
	</xsd:group>
	<xsd:group name="ServiceFacilityGroup">
		<xsd:annotation>
			<xsd:documentation>Facilities that apply to services.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="CommonFacilityGroup"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="AccommodationFacility"/>
		</xsd:sequence>
	</xsd:group>
