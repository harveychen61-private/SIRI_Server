	<!-- ====SERVICE JOURNEY INTERCHANGE ================================== -->
	<xsd:complexType name="InterchangeJourneyStructure">
		<xsd:annotation>
			<xsd:documentation>Type for feeder or Distributor Journey info about a VEHICLE
				JOURNEY.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:group ref="JourneyIdentityGroup"/>
			<xsd:group minOccurs="0" ref="JourneyPatternInfoGroup"/>
			<xsd:group minOccurs="0" ref="VehicleJourneyInfoGroup"/>
			<xsd:group minOccurs="0" ref="DisruptionGroup"/>
			<xsd:group minOccurs="0" ref="OperationalInfoGroup"/>
			<xsd:element form="qualified" minOccurs="0" name="Monitored" type="xsd:boolean">
				<xsd:annotation>
					<xsd:documentation>Whether there is real-time information available for journey;
						if not present, not known.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="AimedDepartureTime"
				type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>On a Distributor journey , a Timetabled departure time of the
						VEHICLE JOURNEY from the CONNECTION LINK for the SERVICE JOURNEY
						INTERCHANGE. On a Feeder journey a Timetabled arrival time of the VEHICLE
						JOURNEY at the CONNECTION link for the SERVICE JOURNEY
						INTERCHANGE.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="Extensions"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<xsd:group name="SiriConnectionTimetableIndexGroup">
		<xsd:annotation>
			<xsd:documentation>Convenience artifact to pick out main elements of the Connection
				Timetable Service.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ConnectionTimetableRequest"/>
			<xsd:element ref="ConnectionTimetableSubscriptionRequest"/>
			<xsd:element ref="ConnectionTimetableDelivery"/>
			<xsd:sequence>
				<xsd:element ref="ConnectionTimetableCapabilitiesRequest"/>
				<xsd:element ref="ConnectionTimetableCapabilitiesResponse"/>
			</xsd:sequence>
		</xsd:sequence>
	</xsd:group>
	<!-- ====REQUESTS ====================================================== -->
	<!-- ===== FS: Connection Timetable Request ================================== -->
	<xsd:element name="ConnectionTimetableRequest"
		substitutionGroup="AbstractFunctionalServiceRequest"
		type="ConnectionTimetableRequestStructure">
		<xsd:annotation>
			<xsd:documentation>Request for information about timetabled connections at a
				stop.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:group name="ConnectionTimetableTopicGroup">
		<xsd:annotation>
			<xsd:documentation>Parameters that specify the content to be
				returned.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element form="qualified" minOccurs="0" name="ArrivalWindow"
				type="ClosedTimestampRangeStructure">
				<xsd:annotation>
					<xsd:documentation>Earliest and latest time. If absent, default to the data
						horizon of the service.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" name="ConnectionLinkRef" type="ConnectionLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>CONNECTION link for which data is to be
						supplied.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="LineRef" type="LineRefStructure">
				<xsd:annotation>
					<xsd:documentation>Feeder LINE for which data is to be
						supplied.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" minOccurs="0" name="DirectionRef"
				type="DirectionRefStructure">
				<xsd:annotation>
					<xsd:documentation>Feeder DIRECTION for which data is to be
						supplied.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:group>
	<xsd:complexType name="ConnectionTimetableRequestStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Request for Connection Timetable
				Service.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractFunctionalServiceRequestStructure">
				<xsd:sequence>
					<xsd:group ref="ConnectionTimetableTopicGroup"/>
					<xsd:group ref="ConnectionTimetableRequestPolicyGroup"/>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
				<xsd:attribute default="2.0" form="unqualified" name="version" type="VersionString">
					<xsd:annotation>
						<xsd:documentation>Version number of request. Fixed </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ConnectionTimetableRequestPolicyGroup">
		<xsd:annotation>
			<xsd:documentation>Parameters that affect the request processing.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element default="en" form="qualified" minOccurs="0" name="Language"
				type="xsd:language">
				<xsd:annotation>
					<xsd:documentation>Preferred language in which to return text
						values.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element minOccurs="0" ref="IncludeTranslations"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ===== FS: Connection Timetable Subscription Request ================================== -->
	<xsd:group name="ConnectionTimetableSunscriptionPolicyGroup">
		<xsd:annotation>
			<xsd:documentation>Parameters that affect the subscription
				processing.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence/>
	</xsd:group>
	<xsd:element name="ConnectionTimetableSubscriptionRequest"
		substitutionGroup="AbstractFunctionalServiceSubscriptionRequest"
		type="ConnectionTimetableSubscriptionStructure">
		<xsd:annotation>
			<xsd:documentation>Subscription Request for information about Timetabled connections at
				a stop.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ConnectionTimetableSubscriptionStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Subscription Request for Connection
				Protection.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractSubscriptionStructure">
				<xsd:sequence>
					<xsd:element ref="ConnectionTimetableRequest"/>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ====RESPONSES=========================================================== -->
	<!-- ===== FS: Connection Timetable Delivery================================== -->
	<xsd:element name="ConnectionTimetableDelivery"
		substitutionGroup="AbstractFunctionalServiceDelivery"
		type="ConnectionTimetableDeliveryStructure">
		<xsd:annotation>
			<xsd:documentation>Delivery for Connection Timetable Service.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ConnectionTimetableDeliveryStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Delivery for Connection Protection.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractServiceDeliveryStructure">
				<xsd:sequence>
					<xsd:group ref="ConnectionTimetablePayloadGroup"/>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
				<xsd:attribute default="2.0" form="unqualified" name="version" type="VersionString">
					<xsd:annotation>
						<xsd:documentation>Version number of response. Fixed </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="ConnectionTimetablePayloadGroup">
		<xsd:annotation>
			<xsd:documentation>Feeder delivery payload content.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element maxOccurs="unbounded" minOccurs="0" ref="TimetabledFeederArrival"/>
			<xsd:element maxOccurs="unbounded" minOccurs="0"
				ref="TimetabledFeederArrivalCancellation"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:element abstract="true" name="FeederItem" type="AbstractFeederItemStructure">
		<xsd:annotation>
			<xsd:documentation>Fedder/Fetcher SERVICE JOURNEY INTERCHANGE
				Activity.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType abstract="true" name="AbstractFeederItemStructure">
		<xsd:annotation>
			<xsd:documentation>Type for an SERVICE JOURNEY INTERCHANGE feeder
				Activity.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractIdentifiedItemStructure">
				<xsd:sequence>
					<xsd:element form="qualified" minOccurs="0" name="ValidUntilTime"
						type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>Time until when data is valid. +SIRI
								2.0</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group ref="InterchangeFeederIdentityGroup"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:group name="InterchangeFeederIdentityGroup">
		<xsd:annotation>
			<xsd:documentation>Elements identifying CALL at CONNECTION link of SERVICE JORUNEY
				INTERCHANGE of Feeder Journey .</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element form="qualified" minOccurs="0" name="InterchangeRef"
				type="InterchangeRefStructure">
				<xsd:annotation>
					<xsd:documentation>Reference to the the SERVICE JOURNEY INTERCHANGE between two
						journeys for which data is being returned.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element form="qualified" name="ConnectionLinkRef" type="ConnectionLinkRefStructure">
				<xsd:annotation>
					<xsd:documentation>Reference to the CONNECTION link or ACCESS ZONE for which
						data is to be returned. i.e. associated with known feeder arrival and
						distributor departure STOP POINTs.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:group ref="ContextualisedStopPointInSequenceGroup"/>
		</xsd:sequence>
	</xsd:group>
	<!-- ======================================================================= -->
	<xsd:element name="TimetabledFeederArrival" substitutionGroup="FeederItem"
		type="TimetabledFeederArrivalStructure">
		<xsd:annotation>
			<xsd:documentation>A feeder arrival at the arrival SCHEDUELD STOP POINT of the
				CONNECTION link .</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TimetabledFeederArrivalStructure">
		<xsd:annotation>
			<xsd:documentation>Type for incoming visit by feeder journey to SERVICE JOURNEY
				NTERCHANGE</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractFeederItemStructure">
				<xsd:sequence>
					<xsd:element form="qualified" name="FeederJourney"
						type="InterchangeJourneyStructure">
						<xsd:annotation>
							<xsd:documentation>Information about the feeder
								journey.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element form="qualified" name="AimedArrivalTime" type="xsd:dateTime">
						<xsd:annotation>
							<xsd:documentation>Planned arrival time at the connection
								point.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="TimetabledFeederArrivalCancellation"
		type="TimetabledFeederArrivalCancellationStructure">
		<xsd:annotation>
			<xsd:documentation>Cancellation of a previously issued Feeder
				Arrival.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="TimetabledFeederArrivalCancellationStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Timetabled Deletion of a feeder
				connection.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractReferencingItemStructure">
				<xsd:sequence>
					<xsd:group ref="InterchangeFeederIdentityGroup"/>
					<xsd:element form="qualified" name="LineRef" type="LineRefStructure">
						<xsd:annotation>
							<xsd:documentation>Reference to a LINE.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element form="qualified" name="DirectionRef" type="DirectionRefStructure">
						<xsd:annotation>
							<xsd:documentation>Reference to a DIRECTION, typically outward or
								return.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element form="qualified" name="VehicleJourneyRef"
						type="FramedVehicleJourneyRefStructure">
						<xsd:annotation>
							<xsd:documentation>Reference to a VEHICLE JOURNEY.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:group minOccurs="0" ref="JourneyPatternInfoGroup"/>
					<xsd:element form="qualified" maxOccurs="unbounded" minOccurs="0" name="Reason"
						type="NaturalLanguageStringStructure">
						<xsd:annotation>
							<xsd:documentation>Reason for deletion. (Unbounded since SIRI
								2.0)</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<!-- ==== WSDL=========================================-->
	<xsd:complexType name="ConnectionTimetableDeliveriesStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Deliveries for Connection Timetable Service. Used in
				WSDl.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element ref="ConnectionTimetableDelivery"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ======================================================================= -->
	<!-- ==== CS: Capability Request=================================================================== -->
	<xsd:element name="ConnectionTimetableCapabilitiesRequest"
		substitutionGroup="AbstractFunctionalServiceCapabilitiesRequest"
		type="ServiceCapabilitiesRequestStructure">
		<xsd:annotation>
			<xsd:documentation>Request for information about Connection Timetable Service
				Capabilities. Answered with a
				ConnectionTimetableCapabilitiesResponse.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ConnectionTimetableCapabilitiesResponse"
		substitutionGroup="AbstractFunctionalServiceCapabilitiesResponse"
		type="ConnectionTimetableCapabilitiesResponseStructure">
		<xsd:annotation>
			<xsd:documentation>Capabilities for Connection Timetable Service. Answers a
				ConnectionTimetableCapabilitiesRequest.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<!-- ==== CS: Capability Response=================================================================== -->
	<xsd:complexType name="ConnectionTimetableCapabilitiesResponseStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Delivery for Connection
				TimetableService.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractServiceCapabilitiesResponseStructure">
				<xsd:sequence>
					<xsd:element minOccurs="0" ref="ConnectionTimetableServiceCapabilities"/>
					<xsd:element form="qualified" minOccurs="0"
						name="ConnectionTimetablePermissions">
						<xsd:annotation>
							<xsd:documentation>Participant's permissions to use the service, Only
								returned if requested.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:complexContent>
								<xsd:extension base="PermissionsStructure">
									<xsd:sequence>
										<xsd:element form="qualified" maxOccurs="unbounded"
											minOccurs="0" name="ConnectionTimetablePermission"
											type="ConnectionServicePermissionStructure">
											<xsd:annotation>
												<xsd:documentation>Permission for a single
												participant or all participants to use an aspect
												of the service.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:extension>
							</xsd:complexContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
				<xsd:attribute default="2.0" form="unqualified" name="version" type="VersionString">
					<xsd:annotation>
						<xsd:documentation> Version number of response. Fixed </xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="ConnectionTimetableCapabilityRequestPolicyStructure">
		<xsd:annotation>
			<xsd:documentation>Type for capability request.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="CapabilityRequestPolicyStructure">
				<xsd:sequence>
					<xsd:element form="qualified" minOccurs="0" name="ForeignJourneysOnly"
						type="xsd:boolean">
						<xsd:annotation>
							<xsd:documentation>Whether results returns foreign journeys
								only.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="ConnectionTimetableServiceCapabilities"
		type="ConnectionTimetableServiceCapabilitiesStructure">
		<xsd:annotation>
			<xsd:documentation>Capabilities of Connection Timetable Service.</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:complexType name="ConnectionTimetableServiceCapabilitiesStructure">
		<xsd:annotation>
			<xsd:documentation>Type for Connection Timetable Capabilities.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="AbstractCapabilitiesStructure">
				<xsd:sequence>
					<xsd:element form="qualified" minOccurs="0" name="TopicFiltering">
						<xsd:annotation>
							<xsd:documentation>Filtering Capabilities.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:sequence>
								<xsd:element ref="FilterByLineRef"/>
								<xsd:element ref="FilterByConnectionLinkRef">
									<xsd:annotation>
										<xsd:documentation>Whether results can be filtered by
											Connection link. Default is ' true'.</xsd:documentation>
									</xsd:annotation>
								</xsd:element>
							</xsd:sequence>
						</xsd:complexType>
					</xsd:element>
					<xsd:element form="qualified" minOccurs="0" name="RequestPolicy">
						<xsd:annotation>
							<xsd:documentation>Request Policy capabilities.</xsd:documentation>
						</xsd:annotation>
						<xsd:complexType>
							<xsd:complexContent>
								<xsd:extension base="CapabilityRequestPolicyStructure">
									<xsd:sequence>
										<xsd:element default="false" form="qualified" minOccurs="0"
											name="ForeignJourneysOnly" type="xsd:boolean">
											<xsd:annotation>
												<xsd:documentation>Whether service returns only
												foreign journeys. Default is
												'false'.</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:extension>
							</xsd:complexContent>
						</xsd:complexType>
					</xsd:element>
					<xsd:element form="qualified" minOccurs="0" name="SubscriptionPolicy"
						type="CapabilitySubscriptionPolicyStructure">
						<xsd:annotation>
							<xsd:documentation>Subscription Policy capabilities.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element form="qualified" minOccurs="0" name="AccessControl"
						type="ConnectionCapabilityAccessControlStructure">
						<xsd:annotation>
							<xsd:documentation>Optional Access control
								capabilities.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element minOccurs="0" ref="Extensions"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ======================================================================= -->
